javascript:((function(){var script=document.createElement("script");script.src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js";document.head.appendChild(script);function get_flvs(a){var c=[];var b=[];a("flashvars").each(function(d,e){c.push(a(e).attr("value").split("&"))});a("embed[flashvars]").each(function(e,d){c.push(a(d).attr("flashvars").split("&"))});a.each(c,function(e,d){a.each(d,function(h,g){var f=g.match(/^(.*?)=(.*)$/);if(["video_url","flvpathValue","file","flv_url"].indexOf(f[1])>-1){b.push(decodeURIComponent(f[2]))}})});alert(b.join("\n"))}window.setTimeout(function(){get_flvs(jQuery.noConflict())},2000);}));
